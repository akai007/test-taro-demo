<view><text class="example-info">uni-forms 组件一般由输入框、选择器、单选框、多选框等控件组成，用以收集、校验、提交数据。</text><uni-forms vue-id="4ee4dc50-1" rules="{{rules}}" value="{{formData}}" validate-trigger="bind" err-show-type="undertext" ref="__r" data-ref="form" onVueInit="__l"><uni-group vue-id="{{('4ee4dc50-2')+','+('4ee4dc50-1')}}" title="基本信息" top="0" onVueInit="__l"><uni-forms-item vue-id="{{('4ee4dc50-3')+','+('4ee4dc50-2')}}" name="name" required="{{true}}" label="用户名" onVueInit="__l"><uni-easyinput onInput="__e" vue-id="{{('4ee4dc50-4')+','+('4ee4dc50-3')}}" type="text" inputBorder="{{true}}" placeholder="请输入用户名" value="{{formData.name}}" data-event-opts="{{[['^input',[['__set_model',['$0','name','$event',[]],['formData']]]]]}}" onVueInit="__l"></uni-easyinput></uni-forms-item><uni-forms-item vue-id="{{('4ee4dc50-5')+','+('4ee4dc50-2')}}" name="age" required="{{true}}" label="年龄" onVueInit="__l"><input class="uni-input-border" type="text" placeholder="请输入年龄" data-event-opts="{{[['blur',[['binddata',['age','$0'],['$event.detail.value']]]],['input',[['__set_model',['$0','age','$event',[]],['formData']]]]]}}" value="{{formData.age}}" onBlur="__e" onInput="__e"/></uni-forms-item><uni-forms-item vue-id="{{('4ee4dc50-6')+','+('4ee4dc50-2')}}" name="email" label="邮箱" onVueInit="__l"><uni-easyinput onInput="__e" vue-id="{{('4ee4dc50-7')+','+('4ee4dc50-6')}}" type="text" placeholder="请输入邮箱" value="{{formData.email}}" data-event-opts="{{[['^input',[['__set_model',['$0','email','$event',[]],['formData']]]]]}}" onVueInit="__l"></uni-easyinput></uni-forms-item><uni-forms-item vue-id="{{('4ee4dc50-8')+','+('4ee4dc50-2')}}" required="{{true}}" name="birth" label="出生日期" onVueInit="__l"><uni-datetime-picker onInput="__e" vue-id="{{('4ee4dc50-9')+','+('4ee4dc50-8')}}" type="date" start="2000-06-01 06:30:30" end="2030-6-1" return-type="timestamp" value="{{formData.birth}}" data-event-opts="{{[['^input',[['__set_model',['$0','birth','$event',[]],['formData']]]]]}}" onVueInit="__l"></uni-datetime-picker></uni-forms-item><uni-forms-item vue-id="{{('4ee4dc50-10')+','+('4ee4dc50-2')}}" name="checked" label="详细信息" onVueInit="__l"><switch checked="{{formData.checked}}" data-event-opts="{{[['change',[['change',['checked','$0'],['$event.detail.value']]]]]}}" onChange="__e"></switch></uni-forms-item></uni-group><block a:if="{{formData.checked}}"><uni-group vue-id="{{('4ee4dc50-11')+','+('4ee4dc50-1')}}" title="详细信息" onVueInit="__l"><uni-forms-item vue-id="{{('4ee4dc50-12')+','+('4ee4dc50-11')}}" required="{{true}}" name="sex" label="性别" onVueInit="__l"><uni-data-checkbox onInput="__e" vue-id="{{('4ee4dc50-13')+','+('4ee4dc50-12')}}" localdata="{{sex}}" value="{{formData.sex}}" data-event-opts="{{[['^input',[['__set_model',['$0','sex','$event',[]],['formData']]]]]}}" onVueInit="__l"></uni-data-checkbox></uni-forms-item><uni-forms-item vue-id="{{('4ee4dc50-14')+','+('4ee4dc50-11')}}" name="country" label="国家" onVueInit="__l"><picker value="{{formData.country}}" range="{{range}}" data-event-opts="{{[['change',[['binddata',['country','$0'],['$event.detail.value']]]]]}}" onChange="__e"><view>{{formData.country===''?'请选择国家':range[formData.country]}}</view></picker></uni-forms-item><uni-forms-item vue-id="{{('4ee4dc50-15')+','+('4ee4dc50-11')}}" required="{{true}}" name="hobby" label="兴趣爱好" onVueInit="__l"><uni-data-checkbox onInput="__e" vue-id="{{('4ee4dc50-16')+','+('4ee4dc50-15')}}" multiple="{{true}}" localdata="{{hobby}}" value="{{formData.hobby}}" data-event-opts="{{[['^input',[['__set_model',['$0','hobby','$event',[]],['formData']]]]]}}" onVueInit="__l"></uni-data-checkbox></uni-forms-item><uni-forms-item vue-id="{{('4ee4dc50-17')+','+('4ee4dc50-11')}}" name="remarks" label="备注" onVueInit="__l"><uni-easyinput onInput="__e" vue-id="{{('4ee4dc50-18')+','+('4ee4dc50-17')}}" type="textarea" maxlength="{{50}}" placeholder="请输入备注" value="{{formData.remarks}}" data-event-opts="{{[['^input',[['__set_model',['$0','remarks','$event',[]],['formData']]]]]}}" onVueInit="__l"></uni-easyinput></uni-forms-item></uni-group></block><view class="example"><button data-event-opts="{{[['tap',[['submitForm',['form']]]]]}}" class="button" onTap="__e">校验表单</button><button data-event-opts="{{[['tap',[['validateField',['form']]]]]}}" class="button" onTap="__e">只校验用户名和邮箱项</button><button data-event-opts="{{[['tap',[['clearValidate',['form','name']]]]]}}" class="button" onTap="__e">移除用户名的校验结果</button><button data-event-opts="{{[['tap',[['clearValidate',['form']]]]]}}" class="button" onTap="__e">移除全部表单校验结果</button><button data-event-opts="{{[['tap',[['resetForm',['$event']]]]]}}" class="button" onTap="__e">重置表单</button></view></uni-forms></view>